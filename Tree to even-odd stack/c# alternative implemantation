public static void valuesAlt(BinNode<int> t, Stack<int> s)
{
    valuesAlt(t, s, 0);
    valuesAlt(t, s, 1);
}
public static void valuesAlt(BinNode<int> t, Stack<int> s, int modulo)
{
    if (t == null)
        return;
    if (t.GetValue() % 2 == modulo)
        s.Push(t.GetValue());
    valuesAlt(t.GetLeft(), s, modulo);
    valuesAlt(t.GetRight(), s, modulo);
}
